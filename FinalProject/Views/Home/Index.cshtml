<h1>Welcome to LRNR!</h1>



<h3>Search</h3>
<form asp-action="WikiSearch">
    <label for="Wikisearch">Seach Wikipedia: </label>
    <input type="text" name="Wikiquery" required />
    <input class="btn-outline-primary" type="submit"  />
</form>


<form action="NewsSearch">
    <label for="News">Seach News: </label>
    <select name="query">
        <option value="Business">Business</option>
        <option value="Politics">Politics</option>
        <option value="Sports">Sports</option>
        <input class="btn-outline-primary" type="submit"  />
    </select>
</form>

